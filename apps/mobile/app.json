{
  "expo": {
    "name": "hotaru",
    "slug": "hotaru",
    "scheme": "hotaru",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon-light.png",
      "resizeMode": "contain",
      "backgroundColor": "#fafafa",
      "dark": {
        "image": "./assets/splash-icon-dark.png",
        "resizeMode": "contain",
        "backgroundColor": "#2a262b"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2a262b"
      },
      "package": "com.luk3mn.hotaru"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
